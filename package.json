    "@graphql-tools/schema": "^10.0.30",
    "@libsql/client": "^0.14.0",
    "@nextui-org/react": "^2.6.11",
    "@urql/exchange-graphcache": "^7.2.1",
    "@urql/next": "^1.1.5",
    "apollo-server-micro": "^3.13.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.3",
    "graphql": "^16.10.0",
    "jest-watch-typeahead": "^2.2.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.475.0",
    "next": "15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sqlite3": "^5.1.7",
    "urql": "^4.2.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/testing-library__user-event": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
